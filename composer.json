{
  "name": "jot/hf-utils",
  "description": "HyperF reimplementations and utils",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "hyperf",
    "elasticsearch",
    "etcd"
  ],
  "autoload": {
    "psr-4": {
      "Jot\\HfUtils\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Joao Zanon"
    }
  ],
  "extra": {
    "hyperf": {
      "config": "Jot\\HfRepository\\ConfigProvider"
    }
  },
  "require": {
    "php": ">=8.1",
    "hyperf/command": "~3.1.0",
    "hyperf/di": "~3.1.0",
    "hyperf/http-server": "^3.1",
    "hyperf/etcd": "^3.1"
  }
}
